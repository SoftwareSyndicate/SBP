<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <icon density="ldpi" src="www/images/sbp_stamp.png" />
    <icon density="mdpi" src="www/images/sbp_stamp.png" />
    <icon density="hdpi" src="www/images/sbp_stamp.png" />
    <icon density="xhdpi" src="www/images/sbp_stamp.png" />
    <icon density="xxhdpi" src="www/images/sbp_stamp.png" />
    <icon density="xxxhdpi" src="www/images/sbp_stamp.png" />
    <allow-intent href="market:*" />
    <name>SBP</name>
    <description>
      Seattle Bouldering Project 
    </description>
    <author email="yintothayang@gmail.com" href="http://makowaredev.com">
      Syndicate Software
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
