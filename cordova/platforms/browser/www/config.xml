<?xml version='1.0' encoding='utf-8'?>
<widget id="software.syndicate.sbp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>SBP</name>
    <description>
    Seattle Bouldering Project
  </description>
    <author email="yintothayang@gmail.com" href="http://syndicate.software">
    Syndicate Software
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="SplashScreenDelay" value="3500" />
    <preference name="orientation" value="portrait" />
</widget>
