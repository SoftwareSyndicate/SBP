<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>SBP</name>
  <description>
    Seattle Bouldering Project 
  </description>
  <author email="yintothayang@gmail.com" href="http://makowaredev.com">
    Syndicate Software
  </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <splash src="www/images/sbp_splash_screen.png" density="land-hdpi"/>
    <splash src="www/images/sbp_splash_screen.png" density="land-ldpi"/>
    <splash src="www/images/sbp_splash_screen.png" density="land-mdpi"/>
    <splash src="www/images/sbp_splash_screen.png" density="land-xhdpi"/>
    <splash src="www/images/sbp_splash_screen.png" density="port-hdpi"/>
    <splash src="www/images/sbp_splash_screen.png" density="port-ldpi"/>
    <splash src="www/images/sbp_splash_screen.png" density="port-mdpi"/>
    <splash src="www/images/sbp_splash_screen.png" density="port-xhdpi"/>
    <icon src="www/images/sbp_stamp.png" density="ldpi" />
    <icon src="www/images/sbp_stamp.png" density="mdpi" />
    <icon src="www/images/sbp_stamp.png" density="hdpi" />
    <icon src="www/images/sbp_stamp.png" density="xhdpi" />
    <icon src="www/images/sbp_stamp.png" density="xxhdpi" />
    <icon src="www/images/sbp_stamp.png" density="xxxhdpi" />
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <preference name="SplashScreenDelay" value="2500" />
  <preference name="orientation" value="portrait" />
</widget>
