<?xml version='1.0' encoding='utf-8'?>
<widget id="software.syndicate.sbp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>SBP</name>
  <description>
    Seattle Bouldering Project
  </description>
  <author email="yintothayang@gmail.com" href="http://syndicate.software">
    Syndicate Software
  </author>
  <content src="index.html" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <preference name="SplashScreenDelay" value="5000" />
  <preference name="FadeSplashScreen" value="true" />
  <preference name="FadeSplashScreenDuration" value="1000" />
  <preference name="orientation" value="portrait" />
  <preference name="StatusBarOverlaysWebView" value="false" />
  <preference name="StatusBarBackgroundColor" value="#000000" />
  <preference name="StatusBarStyle" value="lightcontent" />
  <preference name="DisallowOverscroll" value="true" />
  <preference name="AllowInlineMediaPlayback" value="true" />
  <preference name="MediaPlaybackRequiresUserAction" value="false" />
  <allow-navigation href="*youtube*" />
  <allow-navigation href="*ytimg*" />
  <platform name="android">
    <splash density="land-hdpi" src="www/images/splash.png" />
    <splash density="land-ldpi" src="www/images/splash.png" />
    <splash density="land-mdpi" src="www/images/splash.png" />
    <splash density="land-xhdpi" src="www/images/splash.png" />
    <splash density="port-hdpi" src="www/images/splash.png" />
    <splash density="port-ldpi" src="www/images/splash.png" />
    <splash density="port-mdpi" src="www/images/splash.png" />
    <splash density="port-xhdpi" src="www/images/splash.png" />
    <icon density="ldpi" src="www/images/sbp_stamp.png" />
    <icon density="mdpi" src="www/images/sbp_stamp.png" />
    <icon density="hdpi" src="www/images/sbp_stamp.png" />
    <icon density="xhdpi" src="www/images/sbp_stamp.png" />
    <icon density="xxhdpi" src="www/images/sbp_stamp.png" />
    <icon density="xxxhdpi" src="www/images/sbp_stamp.png" />
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <icon height="180" src="www/images/ios_icons/icon-60@3x.png" width="180" />
    <icon height="60" src="www/images/ios_icons/icon-60.png" width="60" />
    <icon height="120" src="www/images/ios_icons/icon-60@2x.png" width="120" />
    <icon height="76" src="www/images/ios_icons/icon-76.png" width="76" />
    <icon height="152" src="www/images/ios_icons/icon-76@2x.png" width="152" />
    <icon height="40" src="www/images/ios_icons/icon-40.png" width="40" />
    <icon height="80" src="www/images/ios_icons/icon-40@2x.png" width="80" />
    <icon height="120" src="www/images/ios_icons/icon-40@3x.png" width="120" />
    <icon height="57" src="www/images/ios_icons/icon.png" width="57" />
    <icon height="114" src="www/images/ios_icons/icon@2x.png" width="114" />
    <icon height="72" src="www/images/ios_icons/icon-72.png" width="72" />
    <icon height="144" src="www/images/ios_icons/icon-72@2x.png" width="144" />
    <icon height="29" src="www/images/ios_icons/icon-small.png" width="29" />
    <icon height="58" src="www/images/ios_icons/icon-small@2x.png" width="58" />
    <splash height="480" src="www/images/splash.png" width="320" />
    <splash height="960" src="www/images/splash.png" width="640" />
    <splash height="1024" src="www/images/splash.png" width="768" />
    <splash height="2048" src="www/images/splash.png" width="1536" />
    <splash height="768" src="www/images/splash.png" width="1024" />
    <splash height="1536" src="www/images/splash.png" width="2048" />
    <splash height="1136" src="www/images/splash.png" width="640" />
    <splash height="1334" src="www/images/splash.png" width="750" />
    <splash height="2208" src="www/images/splash.png" width="1242" />
    <splash height="1242" src="www/images/splash.png" width="2208" />
  </platform>
  <engine name="android" spec="~5.2.2" />
</widget>
